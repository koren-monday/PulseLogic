# API Endpoint
# For web development:
VITE_API_URL=http://localhost:3002/api
# For Android emulator (uncomment when building Android):
# VITE_API_URL=http://10.0.2.2:3002/api

# Firebase Client Configuration (required for persistent auth)
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id

# RevenueCat (Android only - get from RevenueCat dashboard)
VITE_REVENUECAT_PUBLIC_KEY=your-revenuecat-public-key
